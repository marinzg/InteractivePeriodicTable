src=3D"https://upload.wikimedia.org/wikipedia/commons/thumb/7/73/StericAc=
idChemStr.png/300px-StericAcidChemStr.png"=20
data-file-height=3D"475" data-file-width=3D"1906" =
srcset=3D"//upload.wikimedia.org/wikipedia/commons/thumb/7/73/StericAcidC=
