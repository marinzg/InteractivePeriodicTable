        xmlns:local="clr-namespace:InteractivePeriodicTable"
        mc:Ignorable="d"
        Title="Quiz" Height="300" Width="300">
    <Grid x:Name="g">
        <StackPanel x:Name="sp" Margin="0,41,0,0" />

        <Label x:Name="timer" Content="Left:" HorizontalAlignment="Left" Margin="223,10,0,0" VerticalAlignment="Top"/>
