<Window x:Class="InteractivePeriodicTable.ScoreBoard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InteractivePeriodicTable"
        mc:Ignorable="d"
        Title="ScoreBoard" Height="300" Width="300">
    <Grid>
        <StackPanel x:Name="scoreboard" Margin="0,41,0,49"></StackPanel>
        <Label x:Name="label" Content="Top 10 players" HorizontalAlignment="Left" Margin="104,10,0,0" VerticalAlignment="Top"/>
        <Button x:Name="close" Content="Close" HorizontalAlignment="Left" Margin="110,234,0,0" VerticalAlignment="Top" Width="75" Click="close_Click"/>

    </Grid>
</Window>
